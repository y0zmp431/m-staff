@font-face {
	font-family: HennyPenny;
	src: local("HennyPenny"),
	url(/HennyPenny-Regular.ttf);
}
@font-face {
	font-family: DollhouseC;
	src: local("DollhouseC"),
	url(/pNGnrklh.ttf);
}
@font-face {
	font-family: Gabriela;
	src: local("Gabriela"),
	url(/Gabriela-Regular.ttf);
}

.float_l{
  float:left;
}
.float_r{
  float:right;
}
html, body{
	height:100%;
	background:#fff;
}
body{
	padding:0;
	background:#fff;
	padding:0;
	margin:0;
}
header{
	margin:0 auto;
	padding:0;
	background:#fff;
	height:90px;
}
.header-wrapper{
	width:800px;
	margin:0 auto;
	padding:0;
}
.logo{
	float:left;
	width:270px;
}
.logo hgroup{
	color:#555;
}
.logo hgroup:hover{
	color:#d58;
}
.logo h1{
	font-family: 'HennyPenny', sans-serif;
	font-size: 25pt;
	padding:0;
	margin:25px 0 0 0;
	text-align:center;
	line-height:100%;
}
.logo h2{
	font-family: 'DollhouseC', sans-serif;
	font-size: 10pt;
	padding:0;
	margin:0;
	text-align:center;
}
nav.main{
	display:block;
	float:left;
	margin:13px 0 0 0;
}
nav li{
	display:inline;
	margin: 0 7px;
}
nav.main a{
	font-family: 'Gabriela', serif;
	font-family: 'DollhouseC', serif;
	color:#55d;
	font-size: 15pt;
	text-decoration:none;
}
nav.main a:hover{
	color:#d58;
}
.content{
	clear:both;
  min-height:400px;
  margin:0 auto;
  width:800px;
}
footer{
  clear:both;
	height:300px;
	background:url(<%= asset_path "group_a.jpg" %>) no-repeat left bottom #010101;
	font-family: 'HennyPenny', sans-serif;
	color:#f09;
  width:100%;
}
footer>div{
  margin-left:550px;
	padding:30px 0 0 0;
}
footer h1, footer p{
	font:10pt sans-serif;
}
.sub-content{
	clear:both;
	background: #f30095;
  /*
	background:url(<%= asset_path "pink_h.jpg" %>) no-repeat left bottom #f30095;
  */
	padding:10px;
}
.pink_container{
	background: #f30095;
}
.sub-content .my_width{
  width:480px;
	margin:0 20% 0 50%;
}
.sub-content article{
	margin:80px 0 80px 0;
}
article p{
	color:#eee;
  font-size:12pt;
	line-height:150%;
	font-family:sans-serif;
}
div.my_width{
	width:800px;
	margin:0 auto;
}
img.main{
	display:block;
	width:600px;
	margin:0 auto;
	border:solid 1px #fff;
}

div.preview1{
	float:left;
	font-family: 'HennyPenny', sans-serif;
	color:#fff;
	margin:0 2px;
}
div.preview1 .image{
	width:300px;
	height:200px;
	border-radius:5px;
}
div.preview1 img{
	display:block;
}
div.preview1 h2{
	font:sans-serif;
	font-size:11pt;
	text-align:center;
	margin:0 0;
}
div.preview{
	width:180px;
	margin:10px;
	float:left;
}
div.preview img{
	display:block;
	border-radius:10px;
}
div.preview h2{
	font:sans-serif;
	font-size:11pt;
	text-align:center;
	margin:0 0;
}
div.preview div.female{
	width:20px;
	height:18px;
	background: url(<%= asset_path "female.jpg" %>) no-repeat;
}
div.preview div.male{
	width:20px;
	height:18px;
	background: url(<%= asset_path "male.jpg" %>) no-repeat;
}
p{
	font-size:14px;
}
textarea.small{
  width:300px;
  height:100px;
}
.clear{
	clear:both;
}
/*
width for main img
*/
.blank_for_img, #main_img{
	width:600px;
}
/*
width for content
*/
#content-wrapper, #content, #head, #head-wrapper{
	max-width:600px;
	min-width:500px;
}

.blank_for_img{
	height:100px;
	float:left;
}
#main_img{
	height:100%;
	background: url(<%= asset_path 'bella.png' %>) no-repeat;
	clear:both;
	float:left;
	margin-top:-150px;
}
#head-wrapper{
	float:left;
	height:200px;
}
.headliner{
	float:left;
	display:block;
	width:400px;
	height:1px;
}
#head{
	position:absolute;
	top:0;
	text-align:center;
}
#menu{
	margin:0 auto;
	padding:0;
	/*
	*/
}
#menu li{
	display:inline;
	margin:0 5px 5px 5px;
	list-style-type:none;
	line-height:1.5;
}
#content-wrapper{
	float:left;
	/*
	*/
}
#content{
	position:absolute;
	/*
	border:solid 1px #aa2;
	margin:30px 10px 10px 20px;
	*/
}
#logo{
	background: url(<%= asset_path "logo.jpg" %>) no-repeat;
	width:400px;
	height:150px;
	font-family: 'Comfortaa';
	margin-top:auto -70px;
	margin:0 auto;
	/*
	border:solid 1px #a00;
	float:left;
	margin:0 auto;
	*/
}
#logo h1{
	display:block;
	font-family: 'Comfortaa';
	font-size:12px;
	color:#f9e;
	text-shadow: #f00 0px 0px 5px;
	margin:0;
	padding:30px 0 0 0;
}
#logo h1.logo{
	padding:35px 0 0 65px;
}
#logo h1.left{
	padding:35px 0 0 75px;
}
#logo h1.right{
	text-align:right;
	padding:35px 25px 0 0;
}
#logo h1.center{
	text-align:center;
	padding:35px 0 0 0;
}
#logo h1.afterA{
	padding:35px 0 0 120px;
}
.ui-autocomplete{
  list-style-type:none;
  border-radius:3px;
  border: 1px solid #ccc;
  padding:0;
  background:rgba(255, 255, 255, 0.8);
  width:200px;
}
.ui-autocomplete li a{
  display:block;
  padding:3px;
}
.ui-autocomplete .ui-state-focus{
  background:#ddd;
}
.ui-helper-hidden-accessible{
  color:#aaf;
}
.polaroid{
  padding:10px;
  border-radius:3px;
  border:solid 1px rgba(100,100,100,0.1);
  box-shadow: 3px 3px 5px rgba(0,0,0,0.3);
}
.polaroid.h{
  padding-bottom:30px;
}
.polaroid img{
  border-radius:3px;
}
